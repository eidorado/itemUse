				</param>  
				<param name="maximumSize">5242880</param> <!-- 5M -->
			</interceptor-ref>
			<result name="login" type="redirect">/possw0rddyh0z0sfafafa/hzd1jgongesjgs666zqss1gdlyemianwyhohoho.jsp</result>
