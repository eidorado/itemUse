<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.sanqing.po.SoZoTag" table="t_sozo_tag">
  <id name="id">
   <generator class="increment"/>
  </id>
  <property generated="never" lazy="false" name="tagName"/>
  <property generated="never" lazy="false" name="tagRemak"/>
  <property generated="never" lazy="false" name="pId"/>
 </class>
</hibernate-mapping>
