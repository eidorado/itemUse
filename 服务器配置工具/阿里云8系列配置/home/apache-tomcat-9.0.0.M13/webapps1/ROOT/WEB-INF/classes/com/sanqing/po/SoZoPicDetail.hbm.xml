<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sanqing.po.SoZoPicDetail" table="t_sozo_pic_detail">
		<id name="id">
			<generator class="increment"></generator>
		</id>
		<property name="picId"></property>
		<property name="fileName"></property>
		<property name="filePath"></property>
		<property name="fileBig"></property>
		<property name="fileSuffix"></property>
		<property name="content"></property>
		<property name="flag"></property>
		<property name="sn"></property>
	</class>	
</hibernate-mapping>

