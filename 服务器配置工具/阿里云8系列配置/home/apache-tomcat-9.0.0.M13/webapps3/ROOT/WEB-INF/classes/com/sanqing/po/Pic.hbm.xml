<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sanqing.po.Pic" table="t_pic">  <!-- 不写table 默认Pic表 -->
		<id name="id">
			<generator class="increment"></generator>
		</id>
		<property name="title"></property>
		<property name="secTitle"></property>
		<property name="client"></property>
		<property name="industry"></property>
		<property name="doTime"></property>
		<property name="intro"></property>
		<property name="createrId"></property>
		<property name="createDate"></property>
		<property name="tagIds"></property>
		<property name="tagNames"></property>
		<property name="type"></property>
		<property name="status"></property>
		<property name="sn"></property>
		<property name="picSize"></property>
		<property name="picName"></property>
		<property name="picUrl"></property>
		<property name="picBig"></property>
		<property name="picSuffix"></property>
		<property name="secPicName"></property>
		<property name="secPicUrl"></property>
		<property name="secPicBig"></property>
		<property name="secPicSuffix"></property>
		<property name="littleAPicName"></property>
		<property name="littleAPicUrl"></property>
		<property name="littleAPicBig"></property>
		<property name="littleBPicName"></property>
		<property name="littleBPicUrl"></property>
		<property name="littleBPicBig"></property>
	</class>	
</hibernate-mapping>

